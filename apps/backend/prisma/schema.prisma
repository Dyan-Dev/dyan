generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Endpoint {
  id        String   @id @default(cuid())
  path      String
  method    String
  language  String
  code      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([path, method])
}